FUNCTION main :
READ t1
v1 := t1 
t2 := #0
IF v1 > t2 GOTO label3
GOTO label4
LABEL label3 :
t3 := #1
WRITE t3
GOTO label5
LABEL label4 :
t2 := #0
IF v1 < t2 GOTO label0
GOTO label1
LABEL label0 :
t5 := #1
t4 := #0 - t5
WRITE t4
GOTO label2
LABEL label1 :
t2 := #0
WRITE t2
LABEL label2 :
LABEL label5 :
t2 := #0
RETURN t2
